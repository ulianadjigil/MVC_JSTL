<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>/Views/GeneralPage.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>CountStudent</servlet-name>
        <servlet-class>Controllers.CountStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CountStudent</servlet-name>
        <url-pattern>/CountStudent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BiggestFacultyServlet</servlet-name>
        <servlet-class>Controllers.BiggestFacultyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BiggestFacultyServlet</servlet-name>
        <url-pattern>/BiggestFacultyServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BestStudentServlet</servlet-name>
        <servlet-class>Controllers.BestStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BestStudentServlet</servlet-name>
        <url-pattern>/BestStudentServlet</url-pattern>
    </servlet-mapping>

</web-app>